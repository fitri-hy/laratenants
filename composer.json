{
    "name": "fhylabs/laratenants",
    "description": "Laravel Multi-Tenant Library for Laravel 12",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/routing": "^12.0",
        "illuminate/config": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "MultiTenant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MultiTenant\\MultiTenantServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "FhyLabs",
            "email": "info@fhylabs.com"
        }
    ]
}
